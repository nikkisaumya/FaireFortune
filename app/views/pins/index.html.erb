<div id="masonry-container" class="transitions-enabled centered clearfix masonry">
	<div class="row">
	<% @pins.each do |pin| %>
		<div class="box col3 panel panel-default">
			<%= image_tag pin.image.url(:medium) %><br>
			<div class="panel-body">	
				<%= link_to pin.title , pin %>
			</div>
			<% end %>
		</div>
	</div>
</div>
